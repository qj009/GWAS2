labs(x = "Chromsome", y = expression(-log[10]*P)) +
theme_minimal() +
theme(
plot.title = element_text(size = 20,hjust = 0.5),
axis.text.x = element_text(angle = 60, size = 15, vjust = 0.7),
axis.text.y = element_text(size = 15),
